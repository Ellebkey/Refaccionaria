<h1>Listing sells</h1>

<table>
  <thead>
    <tr>
      <th>Cant</th>
      <th>Totalp</th>
      <th>State</th>
      <th>Product</th>
      <th>Facture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sells.each do |sell| %>
      <tr>
        <td><%= sell.cant %></td>
        <td><%= sell.totalp %></td>
        <td><%= sell.state %></td>
        <td><%= sell.product %></td>
        <td><%= sell.facture %></td>
        <td><%= link_to 'Show', sell %></td>
        <td><%= link_to 'Edit', edit_sell_path(sell) %></td>
        <td><%= link_to 'Destroy', sell, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sell', new_sell_path %>
